doctype html
html
    head
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width,initial-scale=1.0')
    body
        div(class='main')
            p Verifying authorization token...
            div(id='result')
                p
                    span Your Parcel identity is&nbsp;
                    button(id='parcel-id', data-cy='parcel-id') #{parcelId}
                p
                    span You have&nbsp;
                    span(id='document-count', data-cy='document-count') #{documentCount}
                    span &nbsp;documents!
